{
  "name": "n8n-nodes-figmation",
  "version": "1.0.0",
  "description": "n8n custom nodes package for Figma workflow automation. Connects to Figma plugin via WebSocket server to execute 36 Figma API commands.",
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "for dir in src/nodes/*/; do if [ -f \"$dir/figmation.png\" ]; then cp \"$dir/figmation.png\" \"dist/nodes/$(basename \"$dir\")/\"; fi; done",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "jest"
  },
  "keywords": [
    "n8n",
    "figma",
    "figmation",
    "websocket",
    "automation",
    "design",
    "workflow",
    "plugin",
    "mcp",
    "ai"
  ],
  "author": {
    "name": "Dante",
    "email": "datapod.k@gmail.com",
    "url": "https://dante-datalab.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dandacompany/n8n-nodes-figmation.git"
  },
  "bugs": {
    "url": "https://github.com/dandacompany/n8n-nodes-figmation/issues"
  },
  "homepage": "https://github.com/dandacompany/n8n-nodes-figmation#readme",
  "dependencies": {
    "n8n-core": "^1.14.1",
    "n8n-workflow": "^1.82.0",
    "ws": "^8.14.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.5",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/FigmationConnector/FigmationConnector.node.js",
      "dist/nodes/FigmationCommander/FigmationCommander.node.js",
      "dist/nodes/FigmationStrategy/FigmationStrategy.node.js"
    ]
  }
}
